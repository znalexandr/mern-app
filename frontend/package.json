{
  "name": "mern-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --hot --PUBLIC_PATH=/",
    "build": "yarn clean && cross-env NODE_ENV=production webpack --PUBLIC_PATH=/",
    "clean": "rimraf build",
    "ts": "tsc --noUnusedLocals",
    "ts:strict": "tsc --noUnusedLocals --strictFunctionTypes",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:js": "eslint './src/**/*.{ts,tsx}'",
    "lint:css": "stylelint './src/**/*.{ts,tsx}'",
    "bundle-analyzer": "cross-env NODE_ENV=production webpack --BUNDLE_ANALYZER"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged && yarn ts"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint",
      "stylelint",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@babel/runtime": "7.10.5",
    "@emotion/cache": "10.0.29",
    "@emotion/core": "10.0.28",
    "@emotion/styled": "10.0.27",
    "axios": "0.21.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hook-form": "6.2.0",
    "react-router-dom": "5.2.0",
    "react-toastify": "6.0.8"
  },
  "devDependencies": {
    "@babel/cli": "7.10.5",
    "@babel/core": "7.10.5",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/plugin-proposal-export-namespace-from": "7.10.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.10.4",
    "@babel/plugin-proposal-optional-chaining": "7.10.4",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.10.5",
    "@babel/preset-env": "7.10.4",
    "@babel/preset-react": "7.10.4",
    "@babel/preset-typescript": "7.10.4",
    "@commitlint/cli": "9.1.1",
    "@commitlint/config-conventional": "9.1.1",
    "@emotion/babel-preset-css-prop": "10.0.27",
    "@pmmmwh/react-refresh-webpack-plugin": "0.3.3",
    "@svgr/webpack": "5.4.0",
    "@types/react": "16.9.43",
    "@types/react-dom": "16.9.8",
    "@types/react-router": "5.1.8",
    "@types/react-router-dom": "5.1.5",
    "@typescript-eslint/parser": "3.7.0",
    "babel-loader": "8.1.0",
    "copy-webpack-plugin": "6.0.3",
    "cross-env": "7.0.2",
    "css-loader": "4.0.0",
    "eslint": "7.5.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.20.3",
    "eslint-plugin-react-hooks": "4.0.8",
    "file-loader": "6.0.0",
    "html-webpack-plugin": "4.3.0",
    "husky": "4.2.5",
    "ignore-not-found-export-webpack-plugin": "1.0.1",
    "lint-staged": "10.2.11",
    "prettier": "2.0.5",
    "prop-types": "15.7.2",
    "react-refresh": "0.8.3",
    "rimraf": "3.0.2",
    "style-loader": "1.2.1",
    "stylelint": "13.6.1",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "20.0.0",
    "stylelint-order": "4.1.0",
    "typescript": "3.9.7",
    "webpack": "4.44.0",
    "webpack-bundle-analyzer": "3.8.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0",
    "yargs": "15.4.1"
  }
}
